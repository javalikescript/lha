{
  "lha_get_data": {
    "tooltip" : "Gets a thing property value",
    "message0": "get %1",
    "args0": [{
      "type": "field_dropdown",
      "name": "PATH",
      "options": "$getThingPathOptions"
    }],
    "output": null,
    "colour": "$lhaDataColor"
  },
  "lha_set_data": {
    "tooltip" : "Sets a thing property value",
    "message0": "set %1 %2",
    "args0": [{
      "type": "field_dropdown",
      "name": "PATH",
      "options": "$setThingPathOptions"
    }, {
      "type": "input_value",
      "name": "VALUE"
    }],
    "previousStatement": null,
    "nextStatement": null,
    "colour": "$lhaDataColor"
  },
  "lha_watch_data": {
    "tooltip" : "Triggers an action on a thing property value change",
    "message0": "watch %1",
    "args0": [{
      "type": "field_dropdown",
      "name": "PATH",
      "options": "$eventThingPathOptions"
    }],
    "message1": "new: %1",
    "args1": [{
      "type": "field_variable",
      "name": "NEW_VALUE",
      "variable": "value"
    }],
    "message2": "do %1",
    "args2": [{
      "type": "input_statement",
      "name": "DO"
    }],
    "inputsInline": true,
    "colour": "$lhaDataColor"
  },

  "lha_event": {
    "tooltip" : "Triggers an action on an event",
    "message0": "on %1",
    "args0": [{
      "type": "field_dropdown",
      "name": "EVENT",
      "options": [
        [ "never", "-disabled-" ],
        [ "startup", "startup" ],
        [ "polling", "poll" ],
        [ "shutdown", "shutdown" ],
        [ "heartbeat", "heartbeat" ],
        [ "testing", "test" ]
      ]
    }],
    "message1": "do %1",
    "args1": [{
      "type": "input_statement",
      "name": "DO"
    }],
    "colour": "$lhaEventColor"
  },
  "lha_schedule": {
    "tooltip" : "Triggers an action on a schedule",
    "message0": "every %1",
    "args0": [{
      "type": "field_input",
      "name": "VALUE",
      "text": "0 0 * * *"
    }],
    "message1": "do %1",
    "args1": [{
      "type": "input_statement",
      "name": "DO"
    }],
    "colour": "$lhaEventColor"
  },
  "lha_timer": {
    "tooltip" : "Triggers an action after a delay",
    "message0": "set timer %1",
    "args0": [{
      "type": "field_input",
      "name": "NAME",
      "text": "my timer"
    }],
    "message1": "in %1 %2",
    "args1": [{
      "type": "field_input",
      "name": "VALUE",
      "text": "1"
    }, {
      "type": "field_dropdown",
      "name": "SECONDS",
      "options": [
        [ "seconds", "1" ],
        [ "minutes", "60" ],
        [ "hours", "3600" ]
      ]
    }],
    "message2": "do %1",
    "args2": [{
      "type": "input_statement",
      "name": "DO"
    }],
    "inputsInline": true,
    "previousStatement": null,
    "nextStatement": null,
    "colour": "$lhaEventColor"
  },
  "lha_clear_timer": {
    "tooltip" : "Clears a timer",
    "message0": "clear timer %1",
    "args0": [{
      "type": "field_input",
      "name": "NAME",
      "text": "my timer"
    }],
    "previousStatement": null,
    "nextStatement": null,
    "colour": "$lhaEventColor"
  },

