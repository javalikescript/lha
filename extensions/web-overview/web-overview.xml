<app-page id="overview" title="Overview">
  <article class="form">
    <p>
      <label>Temperature:</label>
      <span v-html="internalTemperature">na</span>°C /
      <span v-html="externalTemperature">na</span>°C
    </p>
    <p>
      <label>Humidity:</label>
      <span v-html="internalRelativeHumidity">na</span> % /
      <span v-html="externalRelativeHumidity">na</span> %
    </p>
    <p>
      <label>Pressure:</label>
      <span v-html="pressure">na</span> hPa
    </p>
    <p>
      <label>Light:</label>
      <span v-html="internalLightLevel">na</span> Lux
    </p>
  </page-article>
</app-page>
