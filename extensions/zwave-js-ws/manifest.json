{
  "name": "Z-Wave JS WS",
  "description": "Z-Wave JS WebSocket server integration",
  "version": "0.1",
  "script": "zwave-js-ws.lua",
  "schema": {
    "type": "object",
    "properties": {
      "url": {
        "title": "The WebSocket server URL",
        "type": "string",
        "pattern": "^ws://.+:%d+$",
        "required": true,
        "default": "ws://localhost:3000"
      }
    }
  }
}